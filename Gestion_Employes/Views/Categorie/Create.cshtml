@model GestionEmployes.Models.repositories.Categorie

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Categorie</h4>
<hr />
<div class="row">
    <div class="col-md-4 container">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Fonctionnalite" class="control-label"></label>
                <input asp-for="Fonctionnalite" class="form-control" />
                <span asp-validation-for="Fonctionnalite" class="text-danger"></span>
            </div>
            
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a type="button" class="btn btn-info mt-2" asp-action="Index">Reteur</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
